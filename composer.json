{
    "name": "HDUAAA/flarum-ext-auth-cas",
    "description": "Allow users to log in with CAS.",
    "type": "flarum-extension",
    "keywords": ["authentication"],
    "license": "MIT",
    "authors": [
        {
            "name": "ChristianSwift",
            "email": "topman@dingnaiwen.xyz"
        }
    ],
    "support": {
        "issues": "https://github.com/HDUAAA/flarum-ext-auth-cas/issues",
        "source": "https://github.com/HDUAAA/flarum-ext-auth-cas"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "autoload": {
        "psr-4": {
            "Flarum\\Auth\\CAS\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "杭电CAS登陆插件",
            "icon": {
                "name": "lock",
                "backgroundColor": "#eee",
                "color": "#333"
            }
        }
    }
}
